{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "package.json"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "module": "es2020",
    "composite": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"],
    "noImplicitThis": false,
    "strict": false
  }
}
